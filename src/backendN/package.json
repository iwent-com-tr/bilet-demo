{
  "name": "iwent-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "populate": "tsx src/index.ts --populate",
    "dev": "tsx watch src/index.ts",
    "dev:https": "dotenv -e .env.https -- tsx watch src/index.ts",
    "build": "tsc && tsc-esm-fix dist --ext .js",
    "start": "node --env-file=.env --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"./json-loader.mjs\", pathToFileURL(\"./\"));' dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t iwent-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env iwent-backend",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/qrcode": "^1.5.5",
    "dotenv-cli": "^10.0.0",
    "prisma": "^5.15.0",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/twilio": "^3.19.2",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "meilisearch": "^0.52.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^5.12.2",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "twilio": "^5.8.0",
    "zod": "^3.23.8"
  }
}
