{
  "name": "iwent-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "populate": "tsx src/index.ts --populate",
    "dev": "tsx watch src/index.ts",
    "dev:https": "dotenv -e .env.https -- tsx watch src/index.ts",
    "build": "tsc && tsc-esm-fix dist --ext .js",
    "start": "node --env-file=.env dist/src/index.js",
    
    "docker:build": "docker build -t iwent-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env iwent-backend",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "worker": "tsx src/workers/notification-worker.ts",
    "worker:dev": "tsx watch src/workers/notification-worker.ts",
    "push-cli": "tsx src/lib/push/subscription-manager.cli.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/qrcode": "^1.5.5",
  
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.4",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.4",

    "dotenv-cli": "^10.0.0",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "ts-node": "^10.9.2"

  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@sendgrid/mail": "^8.1.5",
